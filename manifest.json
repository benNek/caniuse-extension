{
  "name": "caniuse.com Extension",
  "version": "0.9.9",
  "manifest_version": 2,
  "description": "Right-click to check CSS property parity between browsers",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "contextMenus"
  ]
}